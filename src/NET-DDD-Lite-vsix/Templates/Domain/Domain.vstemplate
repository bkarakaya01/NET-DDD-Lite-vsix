      <ProjectItem ReplaceParameters="true" TargetFileName="Aggregates\Product\Product.cs">Content/Aggregates/Product/Product.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="Aggregates\Product\ProductImage.cs">Content/Aggregates/Product/ProductImage.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="Products\IProductRepository.cs">Content/Products/IProductRepository.cs</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>
